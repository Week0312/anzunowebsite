{
    "name": "kyoukawebsite",
    "version": "0.1.0",
    "private": true,
    "license": "MIT",
    "engines": {
        "node": ">=20.0.0"
    },
    "scripts": {
        "dev": "NODE_NO_WARNINGS=1 next dev",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "clean": "rm -rf .next && yarn cache clean",
        "test": "NODE_NO_WARNINGS=1 jest --detectOpenHandles --silent",
        "test:watch": "NODE_NO_WARNINGS=1 jest --watch",
        "test:coverage": "NODE_NO_WARNINGS=1 jest --coverage",
        "test:verbose": "NODE_NO_WARNINGS=1 jest --detectOpenHandles --verbose",
        "type-check": "tsc --noEmit",
        "format": "prettier --write \"**/*.{ts,tsx,css,json,md}\"",
        "postinstall": "prisma generate",
        "prisma:generate": "prisma generate",
        "prisma:migrate": "prisma migrate dev",
        "prisma:seed": "ts-node prisma/seed.ts"
    },
    "dependencies": {
        "@prisma/client": "^5.10.2",
        "bcryptjs": "^2.4.3",
        "class-variance-authority": "^0.7.0",
        "clsx": "^2.1.0",
        "critters": "^0.0.25",
        "jsonwebtoken": "^9.0.2",
        "lucide-react": "^0.359.0",
        "next": "14.1.0",
        "nodemailer": "^6.9.12",
        "react": "^18",
        "react-dom": "^18",
        "react-hook-form": "^7.51.1",
        "swiper": "^11.1.0",
        "tailwind-merge": "^2.2.2",
        "tailwindcss-animate": "^1.0.7",
        "zod": "^3.22.4"
    },
    "devDependencies": {
        "@babel/core": "^7.26.10",
        "@babel/preset-env": "^7.26.9",
        "@babel/preset-react": "^7.26.3",
        "@babel/preset-typescript": "^7.27.0",
        "@happy-dom/jest-environment": "^17.4.4",
        "@swc/core": "^1.4.8",
        "@swc/jest": "^0.2.36",
        "@tailwindcss/forms": "^0.5.7",
        "@tailwindcss/typography": "^0.5.10",
        "@testing-library/dom": "^10.4.0",
        "@testing-library/jest-dom": "^6.6.3",
        "@testing-library/react": "^16.3.0",
        "@testing-library/react-hooks": "^8.0.1",
        "@testing-library/user-event": "^14.5.2",
        "@types/bcryptjs": "^2.4.6",
        "@types/jest": "^29.5.14",
        "@types/jsonwebtoken": "^9.0.6",
        "@types/node": "^20",
        "@types/node-localstorage": "^1.3.3",
        "@types/nodemailer": "^6.4.14",
        "@types/react": "^18",
        "@types/react-dom": "^18",
        "@typescript-eslint/eslint-plugin": "^8.15.0",
        "@typescript-eslint/parser": "^8.15.0",
        "autoprefixer": "^10.0.1",
        "eslint": "^8",
        "eslint-config-next": "14.1.0",
        "eslint-plugin-jest": "^27.9.0",
        "eslint-plugin-testing-library": "^6.2.0",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
        "node-localstorage": "^3.0.5",
        "postcss": "^8",
        "prettier": "^3.2.5",
        "prettier-plugin-tailwindcss": "^0.5.12",
        "prisma": "^5.10.2",
        "tailwindcss": "^3.3.0",
        "ts-jest": "^29.3.2",
        "ts-node": "^10.9.2",
        "typescript": "^5"
    },
    "prisma": {
        "seed": "ts-node prisma/seed.ts"
    }
